@prefix : <http://www.semanticweb.org/fypprocess#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/fypprocess#> .

<http://www.semanticweb.org/fypprocess> rdf:type owl:Ontology ;
                                         <http://purl.org/dc/terms/creator> "saad, ismail" ;
                                         <http://purl.org/dc/terms/date> "2025-05-16" ;
                                         <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                         <http://purl.org/dc/terms/rights> "This ontology is licensed under Creative Commons Attribution 4.0 International License (CC BY 4.0)" ;
                                         rdfs:comment "An ontology to represent the FYP process, stakeholders, documents, and activities" ;
                                         rdfs:label "Final Year Project Process Ontology" ;
                                         owl:versionInfo "1.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/date
<http://purl.org/dc/terms/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
<http://www.w3.org/2004/02/skos/core#altLabel> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
<http://www.w3.org/2004/02/skos/core#prefLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/fypprocess#advisorOf
:advisorOf rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasAdvisor ;
           rdfs:domain :Supervisor ;
           rdfs:range :FYP ;
           rdfs:comment "Relates a supervisor to the FYP they advise" ;
           rdfs:label "advisor of" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Connects a supervisor to the FYP they provide guidance for" .


###  http://www.semanticweb.org/fypprocess#approvedBy
:approvedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :approves ;
            rdfs:domain :WeeklyTask ;
            rdfs:range :Coordinator ;
            <http://purl.org/dc/terms/description> "Tracks which coordinator has reviewed and approved the completion of specific weekly tasks" ;
            rdfs:comment "Indicates coordinator approval of completed tasks" ;
            rdfs:label "approved by" ;
            <http://www.w3.org/2004/02/skos/core#definition> "Links a task to the coordinator who approved its completion" .


###  http://www.semanticweb.org/fypprocess#approvedByCommittee
:approvedByCommittee rdf:type owl:ObjectProperty ;
                     owl:inverseOf :approvesProposal ;
                     rdfs:domain :Proposal ;
                     rdfs:range :CapstoneCommittee ;
                     <http://purl.org/dc/terms/description> "Establishes that a project proposal has received formal committee approval, allowing it to proceed" ;
                     rdfs:comment "Links a proposal to the approving committee" ;
                     rdfs:label "approved by committee" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Indicates that a project proposal has been approved by the capstone committee" .


###  http://www.semanticweb.org/fypprocess#approves
:approves rdf:type owl:ObjectProperty ;
          rdfs:domain :Coordinator ;
          rdfs:range :WeeklyTask ;
          rdfs:comment "Relates coordinators to tasks they approve" ;
          rdfs:label "approves" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Connects a coordinator to weekly tasks they have reviewed and approved" .


###  http://www.semanticweb.org/fypprocess#approvesProposal
:approvesProposal rdf:type owl:ObjectProperty ;
                  rdfs:domain :CapstoneCommittee ;
                  rdfs:range :Proposal ;
                  rdfs:comment "Relates committees to proposals they approve" ;
                  rdfs:label "approves proposal" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Connects a committee to project proposals it has formally approved" .


###  http://www.semanticweb.org/fypprocess#belongsToGroup
:belongsToGroup rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasGroupMember ;
                rdfs:domain :Student ;
                rdfs:range :CapstoneGroup ;
                <http://purl.org/dc/terms/description> "Associates individual students with their project group, establishing team membership" ;
                rdfs:comment "Links students to their capstone group" ;
                rdfs:label "belongs to group" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Indicates which group a student belongs to for the FYP" .


###  http://www.semanticweb.org/fypprocess#comesAfterPhase
:comesAfterPhase rdf:type owl:ObjectProperty ;
                 owl:inverseOf :comesBeforePhase ;
                 rdfs:domain :ProjectPhase ;
                 rdfs:range :ProjectPhase ;
                 rdfs:comment "Indicates the reverse sequence of project phases" ;
                 rdfs:label "comes after phase" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Establishes reverse temporal ordering between project phases" .


###  http://www.semanticweb.org/fypprocess#comesBeforePhase
:comesBeforePhase rdf:type owl:ObjectProperty ;
                  rdfs:domain :ProjectPhase ;
                  rdfs:range :ProjectPhase ;
                  <http://purl.org/dc/terms/description> "Defines the sequential relationship between phases, indicating which phase precedes another in the project timeline" ;
                  rdfs:comment "Indicates the sequence of project phases" ;
                  rdfs:label "comes before phase" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Establishes temporal ordering between project phases" .


###  http://www.semanticweb.org/fypprocess#conductedBy
:conductedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :conducts ;
             rdfs:domain :Evaluation ;
             rdfs:range :StakeHolder ;
             <http://purl.org/dc/terms/description> "Links evaluation activities to the stakeholders responsible for conducting them" ;
             rdfs:comment "Links evaluations to stakeholders who conduct them" ;
             rdfs:label "conducted by" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Identifies who performs specific evaluation activities" .


###  http://www.semanticweb.org/fypprocess#conducts
:conducts rdf:type owl:ObjectProperty ;
          rdfs:domain :StakeHolder ;
          rdfs:range :Evaluation ;
          rdfs:comment "Relates stakeholders to evaluations they conduct" ;
          rdfs:label "conducts" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Connects a stakeholder to evaluation activities they are responsible for performing" .


###  http://www.semanticweb.org/fypprocess#containsUseCase
:containsUseCase rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isUseCaseIn ;
                 rdfs:domain :SRS ;
                 rdfs:range :UseCase ;
                 <http://purl.org/dc/terms/description> "Connects the Software Requirements Specification to all use cases that describe system interactions" ;
                 rdfs:comment "Relates an SRS to its use cases" ;
                 rdfs:label "contains use case" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Links an SRS document to the use cases it includes" .


###  http://www.semanticweb.org/fypprocess#coordinatedBy
:coordinatedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :coordinates ;
               rdfs:domain :FYP ;
               rdfs:range :Coordinator ;
               <http://purl.org/dc/terms/description> "Establishes the relationship between the FYP and the coordinator who manages the course and evaluation process" ;
               rdfs:comment "Links an FYP to its coordinator" ;
               rdfs:label "coordinated by" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to the faculty member coordinating the course" .


###  http://www.semanticweb.org/fypprocess#coordinates
:coordinates rdf:type owl:ObjectProperty ;
             rdfs:domain :Coordinator ;
             rdfs:range :FYP ;
             rdfs:comment "Relates coordinators to FYPs they coordinate" ;
             rdfs:label "coordinates" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Connects a coordinator to FYPs they manage and oversee" .


###  http://www.semanticweb.org/fypprocess#coordinatorOf
:coordinatorOf rdf:type owl:ObjectProperty ;
               owl:inverseOf :hasCoordinator ;
               rdfs:domain :Coordinator ;
               rdfs:range :FYP ;
               rdfs:comment "Relates a coordinator to the FYP they coordinate" ;
               rdfs:label "coordinator of" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Connects a coordinator to the FYP course they manage" .


###  http://www.semanticweb.org/fypprocess#created
:created rdf:type owl:ObjectProperty ;
         owl:inverseOf :createdBy ;
         rdfs:domain :Student ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Document
                                    :Poster
                                    :Video
                                  )
                    ] ;
         rdfs:comment "Relates a student to artifacts they created" ;
         rdfs:label "created" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Connects a student to documents, videos, or posters they authored" .


###  http://www.semanticweb.org/fypprocess#createdBy
:createdBy rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Document
                                       :Poster
                                       :Video
                                     )
                       ] ;
           rdfs:range :Student ;
           <http://purl.org/dc/terms/description> "Establishes authorship of project artifacts including documents, videos, and posters, identifying the responsible student" ;
           rdfs:comment "Identifies which student created a specific artifact" ;
           rdfs:label "created by" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Links an artifact to the student who authored or created it" .


###  http://www.semanticweb.org/fypprocess#designsFor
:designsFor rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasDesign ;
            rdfs:domain :DesignDocument ;
            rdfs:range :Requirement ;
            <http://purl.org/dc/terms/description> "Connects design documents to the requirements they implement, showing how requirements are translated into design specifications" ;
            rdfs:comment "Relates design documents to requirements" ;
            rdfs:label "designs for" ;
            <http://www.w3.org/2004/02/skos/core#definition> "Links design documents to the requirements they address" .


###  http://www.semanticweb.org/fypprocess#evaluates
:evaluates rdf:type owl:ObjectProperty ;
           owl:inverseOf :undergoes ;
           rdfs:domain :Evaluation ;
           rdfs:range :FYP ;
           rdfs:comment "Relates evaluations to FYPs they assess" ;
           rdfs:label "evaluates" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Connects evaluation activities to the FYPs they are assessing" .


###  http://www.semanticweb.org/fypprocess#evaluatesPhase
:evaluatesPhase rdf:type owl:ObjectProperty ;
                owl:inverseOf :isEvaluatedIn ;
                rdfs:domain :Evaluation ;
                rdfs:range :ProjectPhase ;
                <http://purl.org/dc/terms/description> "Indicates which project phase is being evaluated in a particular evaluation activity" ;
                rdfs:comment "Links evaluations to specific project phases" ;
                rdfs:label "evaluates phase" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Connects evaluation activities to the project phases they assess" .


###  http://www.semanticweb.org/fypprocess#examines
:examines rdf:type owl:ObjectProperty ;
          owl:inverseOf :isExaminedBy ;
          rdfs:domain :Examiner ;
          rdfs:range :FYP ;
          <http://purl.org/dc/terms/description> "Establishes the evaluation relationship between faculty examiners and the projects they are assigned to evaluate" ;
          rdfs:comment "Links examiners to projects they evaluate" ;
          rdfs:label "examines" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Connects examiners to the FYPs they assess" .


###  http://www.semanticweb.org/fypprocess#externallyEvaluates
:externallyEvaluates rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isExternallyEvaluatedBy ;
                     rdfs:domain :ExternalEvaluator ;
                     rdfs:range :FYP ;
                     <http://purl.org/dc/terms/description> "Establishes the assessment relationship between external evaluators and the projects they evaluate" ;
                     rdfs:comment "Links external evaluators to projects they assess" ;
                     rdfs:label "externally evaluates" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Connects external evaluators to the FYPs they provide independent assessment for" .


###  http://www.semanticweb.org/fypprocess#fulfillsRequirement
:fulfillsRequirement rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isFullfilledBy ;
                     rdfs:domain :UseCase ;
                     rdfs:range :Requirement ;
                     <http://purl.org/dc/terms/description> "Establishes traceability between use cases and the requirements they implement or address" ;
                     rdfs:comment "Relates a use case to requirements it fulfills" ;
                     rdfs:label "fulfills requirement" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Links a use case to the requirements it satisfies" .


###  http://www.semanticweb.org/fypprocess#hasActor
:hasActor rdf:type owl:ObjectProperty ;
          owl:inverseOf :isActorIn ;
          rdfs:domain :UseCase ;
          rdfs:range :Actor ;
          <http://purl.org/dc/terms/description> "Links a use case to all actors (users, systems, or entities) that interact with the system in that scenario" ;
          rdfs:comment "Relates a use case to its actors" ;
          rdfs:label "has actor" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Identifies the actors who participate in a use case" .


###  http://www.semanticweb.org/fypprocess#hasAdvisor
:hasAdvisor rdf:type owl:ObjectProperty ;
            rdfs:domain :FYP ;
            rdfs:range :Supervisor ;
            <http://purl.org/dc/terms/description> "Establishes the supervisory relationship between a Final Year Project and the academic staff member who provides guidance and oversight" ;
            rdfs:comment "Relates a FYP to its academic advisor" ;
            rdfs:label "has advisor" ;
            <http://www.w3.org/2004/02/skos/core#definition> "The relationship between an FYP and its supervising faculty member" .


###  http://www.semanticweb.org/fypprocess#hasAlternateScenario
:hasAlternateScenario rdf:type owl:ObjectProperty ;
                      owl:inverseOf :isAlternateScenarioOf ;
                      rdfs:domain :UseCase ;
                      rdfs:range :AlternateScenario ;
                      <http://purl.org/dc/terms/description> "Connects a use case to scenarios that handle exceptions, errors, or alternative paths in the interaction" ;
                      rdfs:comment "Relates a use case to its alternate scenarios" ;
                      rdfs:label "has alternate scenario" ;
                      <http://www.w3.org/2004/02/skos/core#definition> "Links a use case to alternative or exception flows" .


###  http://www.semanticweb.org/fypprocess#hasCoordinator
:hasCoordinator rdf:type owl:ObjectProperty ;
                rdfs:domain :FYP ;
                rdfs:range :Coordinator ;
                <http://purl.org/dc/terms/description> "Connects the Final Year Project to the coordinator overseeing the FYP process, timeline tracking, and administrative coordination" ;
                rdfs:comment "Relates a FYP to its course coordinator" ;
                rdfs:label "has coordinator" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Links an FYP to the faculty member responsible for coordinating the FYP course" .


###  http://www.semanticweb.org/fypprocess#hasDesign
:hasDesign rdf:type owl:ObjectProperty ;
           rdfs:domain :Requirement ;
           rdfs:range :DesignDocument ;
           rdfs:comment "Relates requirements to design documents that address them" ;
           rdfs:label "has design" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Connects requirements to the design documents that translate them into design specifications" .


###  http://www.semanticweb.org/fypprocess#hasExpertiseIn
:hasExpertiseIn rdf:type owl:ObjectProperty ;
                owl:inverseOf :isExpertiseOf ;
                rdfs:domain :Supervisor ;
                rdfs:range :SupervisorExpertise ;
                <http://purl.org/dc/terms/description> "Indicates the specific technical or research domains in which a supervisor specializes" ;
                rdfs:comment "Links supervisors to their areas of expertise" ;
                rdfs:label "has expertise in" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Connects supervisors to their specialized domains of knowledge" .


###  http://www.semanticweb.org/fypprocess#hasFailureContingency
:hasFailureContingency rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isFailureContingencyFor ;
                       rdfs:domain :FYP ;
                       rdfs:range :FailureContingency ;
                       <http://purl.org/dc/terms/description> "Establishes the relationship between a project and its contingency measures for addressing failure or incompletion" ;
                       rdfs:comment "Links an FYP to its failure contingency plan" ;
                       rdfs:label "has failure contingency" ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to procedures for handling incomplete or unsuccessful outcomes" .


###  http://www.semanticweb.org/fypprocess#hasFinalEvaluation
:hasFinalEvaluation rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isFinalEvaluationOf ;
                    rdfs:domain :FYP ;
                    rdfs:range :FinalEvaluation ;
                    <http://purl.org/dc/terms/description> "Establishes the relationship between a project and its final evaluation, which includes demonstrations and comprehensive report submissions" ;
                    rdfs:comment "Links an FYP to its final evaluation" ;
                    rdfs:label "has final evaluation" ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to the comprehensive evaluation conducted during Capstone II" .


###  http://www.semanticweb.org/fypprocess#hasGroupMember
:hasGroupMember rdf:type owl:ObjectProperty ;
                rdfs:domain :CapstoneGroup ;
                rdfs:range :Student ;
                rdfs:comment "Relates a capstone group to its student members" ;
                rdfs:label "has group member" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Connects a project group to the students who belong to it" .


###  http://www.semanticweb.org/fypprocess#hasLeader
:hasLeader rdf:type owl:ObjectProperty ;
           owl:inverseOf :isLeaderOf ;
           rdfs:domain :FYP ;
           rdfs:range :Student ;
           <http://purl.org/dc/terms/description> "Designates which student team member takes on the leadership role, coordinating team activities and serving as the primary contact" ;
           rdfs:comment "Relates a FYP to its team leader" ;
           rdfs:label "has leader" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Identifies the student who serves as the team leader for the FYP" .


###  http://www.semanticweb.org/fypprocess#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :memberOf ;
           rdfs:domain :CapstoneCommittee ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Coordinator
                                      :Supervisor
                                    )
                      ] ;
           rdfs:comment "Relates committees to their faculty members" ;
           rdfs:label "has member" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Connects a committee to the faculty members who serve on it" .


###  http://www.semanticweb.org/fypprocess#hasMidSemesterEvaluation
:hasMidSemesterEvaluation rdf:type owl:ObjectProperty ;
                          owl:inverseOf :isMidSemesterEvaluationOf ;
                          rdfs:domain :FYP ;
                          rdfs:range :MidSemesterEvaluation ;
                          <http://purl.org/dc/terms/description> "Establishes the relationship between a project and its mid-semester evaluation, which includes poster displays and demonstrations" ;
                          rdfs:comment "Links an FYP to its mid-semester evaluation" ;
                          rdfs:label "has mid-semester evaluation" ;
                          <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to the mid-term evaluation conducted during Capstone I" .


###  http://www.semanticweb.org/fypprocess#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain :ProjectPhase ;
                rdfs:range :Student ;
                rdfs:comment "Relates project phases to students who participate in them" ;
                rdfs:label "has participant" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Connects a project phase to students who are involved in its activities" .


###  http://www.semanticweb.org/fypprocess#hasPhase
:hasPhase rdf:type owl:ObjectProperty ;
          owl:inverseOf :isPhaseOf ;
          rdfs:domain :FYP ;
          rdfs:range :ProjectPhase ;
          <http://purl.org/dc/terms/description> "Connects the Final Year Project to its various development phases such as planning, implementation, and testing" ;
          rdfs:comment "Relates a FYP to its project phases" ;
          rdfs:label "has phase" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Links an FYP to the different phases it goes through during its lifecycle" .


###  http://www.semanticweb.org/fypprocess#hasPostCondition
:hasPostCondition rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isPostConditionOf ;
                  rdfs:domain :UseCase ;
                  rdfs:range :PostCondition ;
                  <http://purl.org/dc/terms/description> "Connects a use case to the system states or conditions that exist after the use case completes successfully" ;
                  rdfs:comment "Relates a use case to its post conditions" ;
                  rdfs:label "has post condition" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Links a use case to the conditions that must be true after execution" .


###  http://www.semanticweb.org/fypprocess#hasPoster
:hasPoster rdf:type owl:ObjectProperty ;
           owl:inverseOf :isPosterOf ;
           rdfs:domain :FYP ;
           rdfs:range :Poster ;
           <http://purl.org/dc/terms/description> "Associates a project with its poster presentation, used for exhibitions and evaluations" ;
           rdfs:comment "Links an FYP to its poster presentation" ;
           rdfs:label "has poster" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to its poster, which visually summarizes the project" .


###  http://www.semanticweb.org/fypprocess#hasProjectType
:hasProjectType rdf:type owl:ObjectProperty ;
                owl:inverseOf :isProjectTypeOf ;
                rdfs:domain :FYP ;
                rdfs:range :ProjectType ;
                <http://purl.org/dc/terms/description> "Categorizes an FYP as either a development project or a research project, which affects its methodology and deliverables" ;
                rdfs:comment "Links an FYP to its project type" ;
                rdfs:label "has project type" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Indicates whether a project is development-focused or research-focused" .


###  http://www.semanticweb.org/fypprocess#hasRequirement
:hasRequirement rdf:type owl:ObjectProperty ;
                owl:inverseOf :isRequirementIn ;
                rdfs:domain :SRS ;
                rdfs:range :Requirement ;
                <http://purl.org/dc/terms/description> "Connects the Software Requirements Specification to all functional and non-functional requirements defined within it" ;
                rdfs:comment "Relates an SRS to its requirements" ;
                rdfs:label "has requirement" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Links an SRS document to the requirements it contains" .


###  http://www.semanticweb.org/fypprocess#hasScenario
:hasScenario rdf:type owl:ObjectProperty ;
             owl:inverseOf :isScenarioOf ;
             rdfs:domain :UseCase ;
             rdfs:range :Scenario ;
             <http://purl.org/dc/terms/description> "Connects a use case to the main scenario that describes the normal, expected sequence of interactions" ;
             rdfs:comment "Relates a use case to its main scenario" ;
             rdfs:label "has scenario" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Links a use case to its primary flow of events" .


###  http://www.semanticweb.org/fypprocess#hasTeamMember
:hasTeamMember rdf:type owl:ObjectProperty ;
               owl:inverseOf :isMemberOf ;
               rdfs:domain :FYP ;
               rdfs:range :Student ;
               <http://purl.org/dc/terms/description> "This property links a Final Year Project to all students who are members of the project team, indicating their participation in the project" ;
               rdfs:comment "Relates a FYP to its team members" ;
               rdfs:label "has team member" ;
               <http://www.w3.org/2004/02/skos/core#definition> "A relationship that connects an FYP to the students who are working on it as team members" .


###  http://www.semanticweb.org/fypprocess#hasUser
:hasUser rdf:type owl:ObjectProperty ;
         owl:inverseOf :userOf ;
         rdfs:domain :FYP ;
         rdfs:range :User ;
         rdfs:comment "Relates an FYP to its end users" ;
         rdfs:label "has user" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Connects an FYP to the users of the system it develops" .


###  http://www.semanticweb.org/fypprocess#hasVideo
